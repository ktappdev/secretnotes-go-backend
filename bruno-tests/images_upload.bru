meta {
  name: Images - Upload
  type: http
  seq: 6
}

post {
  url: {{baseUrl}}/api/secretnotes/notes/image
  body: multipartForm
  auth: none
}

headers {
  X-Passphrase: {{testPhrase}}
}

body:multipart-form {
  image: @file(test-image.png)
}

tests {
  test("Status is 200", function() {
    expect(res.getStatus()).to.equal(200);
  });
  
  test("Response contains message", function() {
    expect(res.getBody()).to.have.property('message');
  });
}
