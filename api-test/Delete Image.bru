meta {
  name: Delete Image
  type: http
  seq: 8
}

delete {
  url: {{baseUrl}}/api/secretnotes/notes/{{testPhrase}}/image
  body: none
  auth: none
}

tests {
  test("Status is 200 or 404", function() {
    expect([200, 404]).to.include(res.getStatus());
  });
  
  test("If successful, response contains success message", function() {
    if (res.getStatus() === 200) {
      expect(res.getBody()).to.have.property('message');
      expect(res.getBody().message).to.equal('Image deleted successfully');
    }
  });
}
